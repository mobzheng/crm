<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.sm.crm.mapper.DepartmentMapper">
    <resultMap id="department" type="com.sm.crm.entity.Department">
        <id property="depaId" column="depa_id"/>
        <result property="depaName" column="depa_name"/>
    </resultMap>

    <insert id="insertLog" parameterType="com.sm.crm.entity.Log">
           insert into log(user_name,visit_model,visit_time,behaviour) VALUES(#{userName},#{visitModel},now(),#{behaviour})
    </insert>

    <select id="findAllDepm" resultMap="department">
        SELECT * FROM department
    </select>

</mapper>